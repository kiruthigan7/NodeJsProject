CREATE DATABASE joes_app_node;
USE joes_app_node;
 
CREATE TABLE users(
ID INT AUTO_INCREMENT PRIMARY KEY,
NAME VARCHAR(150),
AGE INT,
CITY VARCHAR(50)
);
 
SELECT * FROM users;
 
